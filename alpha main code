var buttons = [stats_button, reset_button, switch_button, crystals_button];

var ship_list = [
  [101,201,202,301,302,303,304,401,402,403,404,501,502,503,504,601,602,603,604,606,607,608,609,610,611,612,613,614,615,616,617,618,619,620,621,622,623,624,701,702,703,704]
];
  
log = function(s){
game.modding.terminal.echo(s);
};
playerList = function(){
for(nn=0;nn<game.ships.length;nn++)
log(nn+" - "+game.ships[nn].name);
};

playerID = function() {
for (ship of game.ships) echo(ship.id + " : " + ship.name);
};

setAll = function() {
for (ship of game.ships) ship.set({type:101, shield:50000});
};

instructorSaysAll = function(saysWhat, instructor = "Zoltar"){
    for(ship of game.ships) ship.instructorSays(saysWhat, instructor);
};

hideInstructor = function(){
    for(ship of game.ships) ship.hideInstructor();
};

kick = function(ID, reason){
  game.findShip(ID).gameover({"kicked for":reason});
};

this.tick = function(game) {
  if ( game.step % 30 == 0 ) {
    for (let ship of game.ships) {
      if (!ship.custom.buttons_installed && ship.alive)
      {
        ship.custom.buttons_installed = true;
        for (b in buttons)
        {
          ship.setUIComponent(buttons[b]);
          ship.custom.tree = 0;
          for (let tree = 0; tree < ship_list.length; tree++) {
            if (ship_list[tree].indexOf(ship.type) >= 0) {
              ship.custom.tree = tree;
              break;
            }
          }
        }
      }
    }
    for ( var a = 0 ; a < game.ships.length ; a++ ) {
      var admin = game.ships[0];
      if(!admin.custom.god_button_installed) {
        admin.custom.god_button_installed = true;
        admin.setUIComponent(god_button);
      }
    }
  }
  var a = {
    from: 1,
    to: 100
  };
  var b = {
    from: 107,
    to: 200
  };
  var tier7 = [a,b];
  for (let ship of game.ships) {
    if (ship.stats == 0 && [tier7].includes(ship.type)) {
      ship.set({stats: 10000000});
    }
  }
  
Object.keys(alien_info).forEach(key => {
    if (game.step % key === 0) {
      spawnAlien(key);
    }
  });

};

const pos = Array.from({ length: this.options.map_size * 10 }, (_, i) => -this.options.map_size * 5 + i);
const alien_info = {
  500: { // idk secs
    code: 10, // chicken
    number: 3,
    level: 0 // 3max
  },
  3600: { // 1 min
    code: 19, // saucer
    number: 1,
    level: 0 // 3max
  },
  18000: { // 5 min
    code: 12, // fortress
    number: 1,
    level: 0 // 3max
  },
  2700: { // 45 secs
    code: 10, // chicken
    number: 3,
    level: 1 // 3max
  }
};

function spawnAlien(key) {
  const alien = alien_info[key];
  for (let i = 0; i<alien.number; i++) {
    game.addAlien({
      code: alien.code, 
      level: alien.level, 
      x: pos[Math.floor(Math.random() * pos.length)], 
      y: pos[Math.floor(Math.random() * pos.length)]
    });
  }
}



this.event = function(event, game) {
  switch (event.name)
  {
    case "ship_destroyed":
      if (event.killer != null)
      {
        event.killer.custom.kills = event.killer.custom.kills + 1 || 1;
      }
      break;
    case "ui_component_clicked":
      var ship = event.ship;
      var component = event.id;
      switch (component)
      {
        case "switch":
          var tree = ship.custom.tree;
          var index = -1;
          index = ship_list[tree].indexOf(ship.type);
          if (index >= 0) {
            var new_type;
            var new_stats = 0;
            index = (index + 1) % ship_list[tree].length;
            new_type = ship_list[tree][index];
            if (ship.custom.keep_maxed)
            {
              var max = 88888888;
              if (ship.stats < max) {
                new_stats = 88888888;
              }
            }
            ship.set({type:new_type,stats:new_stats,shield:9999});
          }
          break;
        case "stats":
          var stats = ship.stats;
          if (stats < 11111111) {
            ship.custom.keep_maxed = true;
            ship.set({stats:88888888})
          }
          else {
            ship.custom.keep_maxed = false;
            ship.set({stats:00000000})
          }
          break;
        case "reset":
          var new_stats = ship.custom.keep_maxed ? 11111111 : 0;
          ship.set({type:101,stats:new_stats});
          break;
        case "crystals":
          var cargo = 980;
          switch (true) {
            case ship.type >= 101 && ship.type <= 199:
              cargo = 20;
              break;
            case ship.type >= 201 && ship.type <= 299:
              cargo = 80;
              break;
            case ship.type >= 301 && ship.type <= 399:
              cargo = 180;
              break;
            case ship.type >= 401 && ship.type <= 499:
              cargo = 320;
              break;
            case ship.type >= 501 && ship.type <= 599:
              cargo = 500;
              break;  
            case ship.type >= 601 && ship.type <= 699:
              cargo = 720;
              break; 
            case ship.type >= 701 && ship.type <= 799:
              cargo = 980;
              break; 
          }
          ship.set({crystals:cargo,shield:9999});
          break;
        case "admin":
          var new_type;
          if (ship.type == 102) {
            new_type = 792;
          } else if (ship.type == 792) {
            new_type = 793;
          } else if (ship.type == 793) {
            new_type = 791;
          } else {
            new_type = 791;
          }
          ship.set({type:new_type});
          break;
      }
      break;
  }

};
