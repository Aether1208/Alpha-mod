let god_button = {
  id: "admin",
  position: [75,1,2.5,4],
  clickable: false,
  shortcut: "Z",
  visible: false,
  components: [
    { type:  "box",position:[0,0,100,100],fill:"rgba(68, 85, 102, 0)",stroke:"rgba(204, 221, 238, 0.667)",width:2},
